{
  "name": "ember-cli-bugsnag-reporter",
  "version": "0.6.0",
  "description": "Integrates Bugsnag reporting service into your Ember CLI app.",
  "keywords": [
    "ember-addon",
    "bugsnag",
    "error reporting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appaloosa-store/ember-cli-bugsnag-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/appaloosa-store/ember-cli-bugsnag-reporter/issues"
  },
  "homepage": "https://github.com/appaloosa-store/ember-cli-bugsnag-reporter/blob/master/README.MD",
  "license": "Apache-2.0",
  "author": "appaloosa",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js app config tests",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
